let cpstak x y z = 
  (
    let rec tak x y z k = 
    (
      if (not (y < x)) then (k z)
      else 
	tak (x - 1) y z 
	(fun v1 -> 
	  (tak (y - 1) z x 
	    (fun v2 ->
	      tak (z - 1) x y 
		(fun v3 -> 
		  (tak v1 v2 v3 k) 
		)
	    )
	  )
	)
    )
    in
    (tak x y z (fun a -> a))
  ) 
in 
cpstak 32 15 8
