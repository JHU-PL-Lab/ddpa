let phi x1 x2 x3 x4 =
(x1 or x2) and (x1 or (not x2) or (not x3)) and (x3 or x4) and ((not x4) or x1)
  and ((not x2) or (not x3)) and (x4 or x2)
in
let try f =
  (f true) or (f false)
in
let sat_solve_4 p =
  try (fun n1 ->
  try (fun n2 ->
  try (fun n3 ->
  try (fun n4 ->
    p n1 n2 n3 n4
  
))))
in
sat_solve_4 phi
