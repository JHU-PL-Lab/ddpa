# bug in interpreter

let debug_trace x = false in
let id elm =
  (let dum = debug_trace in
  elm)
in
let my_map f l =
  let dum = debug_trace 0 in
  let rec lp lst =
    match lst with
    | [] -> []
    | hd :: tl -> ((id f) hd) :: (lp tl)
  in
  lp l
in
let lst1 = [1, 2, 3] in
let lst2 = [7, 8, 9] in
