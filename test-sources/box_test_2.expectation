QUERY: ~result;
ANALYSES: 0DDPA, 0PLUME, 1PLUME, SPLUME, OSKPLUME, OSMPLUME;
RESULTS: 0DDPA(~result) = `{false, true}`,
         0PLUME(~result) = `{false, true}`,
         1PLUME(~result) = `{true}`,
         SPLUME(~result) = `{true}`,
         OSKPLUME(~result) = `{true}`,
         OSMPLUME(~result) = `{true}`;
CONSISTENCIES: 0DDPA = NO_INCONSISTENCIES &
               0PLUME = NO_INCONSISTENCIES &
               1PLUME = NO_INCONSISTENCIES &
               SPLUME = NO_INCONSISTENCIES &
               OSKPLUME = NO_INCONSISTENCIES &
               OSMPLUME = NO_INCONSISTENCIES;
WELL_FORMED;
EVALUATE;
